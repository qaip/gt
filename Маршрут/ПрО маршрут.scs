SectionSequencesDomain = [*
	section_subject_domain_of_sequences => nrel_main_idtf:
		[Раздел.Предметная область маршрутов](* <-lang_ru;; *);;
	
	section_subject_domain_of_sequences => main_idtf:
		[Section.Subject domain of sequences](*<-lang_en;; *);;
		
	section_subject_domain_of_sequences<-non_atomic_section;;
	
	section_subject_domain_of_sequences<=nrel_section_decomposition:{
		section_subject_domain_of_spanning_sequence;
		section_subject_domain_of_directed_sequence;
	};;
	
	section_subject_domain_of_sequences->rrel_key_sc_element:subject_domain_of_sequences;
		rrel_arc;
		rrel_length_of_sequence;
		rrel_sequence;
		rrel_path;
		
		nrel_adjacent_arcs;
		nrel_semiroute_in_the_digraph;
		
		concept_sequence;
		concept_path;
		concept_tightened_graph;
		concept_arc;
        concept_vertex;
        concept_graph;;
		
	subject_domain_of_sequences => nrel_main_idtf:
		[Предметная область маршрутов](* <-lang_ru;; *);;
		
	subject_domain_of_sequences => nrel_main_idtf:
		[Subject domain of sequences](*<-lang_en;; *);;
		
	subject_domain_of_sequences->rrel_maximum_studied_object_class:concept_graph;;
	subject_domain_of_sequences->rrel_not_maximum_studied_object_class:concept_vertex;;
	
	subject_domain_of_directed_graph=>nrel_private_subject_domain:subject_domain_of_directed_graph;
        subject_domain_of_spanning_sequence;
        section_subject_domain_of_directed_sequence;;
		
	subject_domain_of_sequences <= nrel_private_subject_domain:subject_domain_of_path;;
	
	subject_domain_of_sequences->rrel_explored_relation:
		rrel_arc;
		rrel_length_of_sequence;
		rrel_sequence;
		rrel_path;
		
		nrel_adjacent_arcs;
		nrel_semiroute_in_the_digraph;
		
	subject_domain_of_sequences<-subject_domain;;
	*];;
	
sc_node_struct->
	section_subject_domain_of_sequences;
	subject_domain_of_graph;
	subject_domain_of_sequences;
	section_subject_domain_of_spanning_sequence;
	subject_domain_of_spanning_sequence;
	subject_domain_of_directed_sequence;
	section_subject_domain_of_directed_sequence;;
	
sc_node_role_relation->
	rrel_length_of_sequence;
	rrel_sequence;;

sc_node_norole_relation->
	nrel_semiroute_in_the_digraph;;

	
sc_node_class->
	concept_sequence;
	concept_tightened_graph;
