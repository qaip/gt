	=> nrel_main_idtf:
		[Опр. (трансверсаль*)] (* <- lang_ru;; *);
		[Def. (transvinsal*)] (* <- lang_en;; *);;

	=> nrel_using_constants: {
		concept_hypergraph;
		concept_hypergraph_edge
	};;
*);;

statement -> statement_of_nrel_transvinsal_statement (*
	-> rrel_key_sc_element: nrel_transvinsal;;

	<= nrel_sc_text_translation: ... (*
