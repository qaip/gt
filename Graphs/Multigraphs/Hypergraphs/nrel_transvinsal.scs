		-> [A transversal* of a hypergraph is minimal if none of its subsets is a transversal*] (* <- lang_en;; *);;
	*);;

	=> nrel_main_idtf:
		[Утв. (Высказывание о трансверсали*)] (* <- lang_ru;; *);
		[Stat. (Transversal* statement)] (* <- lang_en;; *);;

	=> nrel_using_constants: {
		concept_hypergraph
	};;
*);;

