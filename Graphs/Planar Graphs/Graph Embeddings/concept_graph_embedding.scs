		[Stat. (Embeddings of Complete Bipartite Graphs)] (* <- lang_en;; *);;

	=> nrel_using_constants: {
		concept_complete_graph;
		concept_bipartite_graph;
		concept_genus;
		nrel_face
	};;
*);;

