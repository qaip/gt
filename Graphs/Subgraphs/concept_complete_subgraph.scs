	[полный подграф] (* <- lang_ru;; *);
	[complete subgraph] (* <- lang_en;; *);;

definition -> definition_of_complete_subgraph (*
	-> rrel_key_sc_element: concept_complete_subgraph;;

	<= nrel_sc_text_translation: ... (*
		-> [Подграф, каждая вершина которого связана со всеми остальными вершинами] (* <- lang_ru;; *);;
		-> [A subgraph where every vertex is connected to every other vertex] (* <- lang_en;; *);;
	*);;

	=> nrel_main_idtf:
		[Опр. (полный подграф)] (* <- lang_ru;; *);
		[Def. (complete subgraph)] (* <- lang_en;; *);;

	=> nrel_using_constants: {
		concept_graph;
		concept_vertex;
		concept_edge;
		concept_subgraph;
		concept_connectivity;
		rrel_vertex;
		rrel_edge
	};;
*);;

statement -> statement_of_complete_graph_theorem (*
	-> rrel_key_sc_element: concept_complete_subgraph;;

	<= nrel_sc_text_translation: ... (*
		-> [Полный граф на n вершинах имеет n(n-1) делить на 2 ребер] (* <- lang_ru;; *);;
		-> [A complete graph on n vertices has n(n-1) divided by 2 edges] (* <- lang_en;; *);;
	*);;

	=> nrel_main_idtf:
		[Утв. (Теорема о полном графе)] (* <- lang_ru;; *);
		[Stat. (Complete graph theorem)] (* <- lang_en;; *);;

	=> nrel_using_constants: {
		concept_graph;
		concept_complete_graph;
		concept_vertex;
		concept_edge;
		rrel_vertex;
		rrel_edge
	};;
*);;

